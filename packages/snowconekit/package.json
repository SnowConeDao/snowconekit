{
  "name": "@sirbenchalot/snowconekit",
  "version": "0.0.1",
  "description": "SnowCones Web3 Connector",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "src/css/reset.css.ts"
  ],
  "engines": {
    "node": ">=12.4"
  },
  "scripts": {
    "build": "node build.js",
    "build:watch": "node build.js --watch",
    "dev": "pnpm build:watch & pnpm typegen:watch",
    "prepare": "MINIFY_CSS=true pnpm build",
    "prebuild": "pnpm typegen",
    "typecheck": "pnpm tsc --noEmit",
    "typegen": "tsc --emitDeclarationOnly || true",
    "typegen:watch": "tsc --emitDeclarationOnly --watch"
  },
  "keywords": [
    "ens",
    "ethereum",
    "react",
    "react-hook",
    "hooks",
    "blockchain"
  ],
  "author": "SnowCone",
  "license": "MIT",
  "peerDependencies": {
    "ethers": ">=5.5.1",
    "react": ">=17",
    "react-dom": ">=17",
    "wagmi": "^0.4.2"
  },
  "devDependencies": {
    "@ethersproject/abstract-provider": "^5.5.1",
    "@ethersproject/providers": "^5.5.1",
    "@types/qrcode": "^1.4.2",
    "@vanilla-extract/private": "^1.0.2",
    "autoprefixer": "^10.4.0",
    "react-dom": "^17.0.2",
    "ethers": "^5.0.0",
    "nock": "^13.2.4",
    "postcss": "^8.4.4",
    "react": "^18.0.0",
    "vitest": "^0.5.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.2",
    "@vanilla-extract/css": "1.7.0",
    "@vanilla-extract/dynamic": "2.0.2",
    "@vanilla-extract/sprinkles": "1.4.1",
    "clsx": "1.1.1",
    "qrcode": "1.5.0",
    "react-remove-scroll": "2.5.3",
    "react-spinners-kit": "^1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sirbenchalot757/snowconekit",
    "directory": "packages/snowconekit"
  }
}